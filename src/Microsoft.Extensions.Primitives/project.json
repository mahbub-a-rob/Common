{
  "version": "1.2.0-*",
  "description": "Primitives shared by framework extensions. Commonly used types include:\r\nMicrosoft.Extensions.Primitives.IChangeToken\r\nMicrosoft.Extensions.Primitives.StringValues\r\nMicrosoft.Extensions.Primitives.StringSegment",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/common"
    },
    "tags": [
      "primitives"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "allowUnsafe": true,
    "compile": "../Microsoft.Extensions.HashCodeCombiner.Sources/**/*.cs"
  },
  "dependencies": {
    "NETStandard.Library": "1.6.1-*",
    "System.Runtime.CompilerServices.Unsafe": "4.3.0-*"
  },
  "frameworks": {
    "netstandard1.0": {}
  }
}